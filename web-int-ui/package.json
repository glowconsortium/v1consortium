{
	"name": "web-int-ui",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"generate:backendapi-axios": "pnpm exec openapi -i backendapi.json -o src/lib/backendapi/generated-backendapi --client axios"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^4.1.1",
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.33.0",
		"@playwright/test": "^1.55.0",
		"@skeletonlabs/skeleton": "^3.2.0",
		"@skeletonlabs/skeleton-svelte": "^1.5.0",
		"@storybook/addon-a11y": "^9.1.3",
		"@storybook/addon-docs": "^9.1.3",
		"@storybook/addon-svelte-csf": "^5.0.8",
		"@storybook/addon-vitest": "^9.1.3",
		"@storybook/sveltekit": "^9.1.3",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.36.1",
		"@sveltejs/vite-plugin-svelte": "^6.1.3",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.12",
		"@types/node": "^22.17.2",
		"@vitest/browser": "^3.2.4",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-storybook": "^9.1.3",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"playwright": "^1.55.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"storybook": "^9.1.3",
		"svelte": "^5.38.2",
		"svelte-check": "^4.3.1",
		"tailwindcss": "^4.1.12",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.40.0",
		"vite": "^7.1.3",
		"vite-plugin-devtools-json": "^0.2.1",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^0.1.0"
	},
	"dependencies": {
		"@auth0/auth0-spa-js": "^2.5.0",
		"@bufbuild/buf": "^1.57.2",
		"@bufbuild/protobuf": "^2.9.0",
		"@bufbuild/protoc-gen-es": "^2.9.0",
		"@connectrpc/connect": "^2.1.0",
		"@connectrpc/connect-web": "^2.1.0",
		"@inlang/paraglide-js": "^2.2.0",
		"@movsm/v1-consortium-web-pkg": "^0.0.7",
		"axios": "^1.11.0",
		"form-data": "^4.0.4",
		"lucide-svelte": "^0.534.0",
		"openapi-typescript-codegen": "^0.29.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}